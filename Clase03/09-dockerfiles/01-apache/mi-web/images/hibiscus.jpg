<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="hibiscusBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00b894;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00cec9;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="hibiscusPetal" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e17055;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#d63031;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#a4161a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="hibiscusCenter" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fff700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e17055;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fondo -->
  <rect width="400" height="300" fill="url(#hibiscusBg)"/>
  
  <!-- Hibiscus principal -->
  <g transform="translate(200,150)">
    <!-- Pétalos del hibiscus (5 pétalos grandes) -->
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(0)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(72)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(144)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(216)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(288)"/>
    
    <!-- Centro de la flor con estambre largo característico -->
    <circle cx="0" cy="0" r="15" fill="url(#hibiscusCenter)"/>
    <rect x="-2" y="-40" width="4" height="40" fill="#d63031"/>
    <circle cx="0" cy="-40" r="6" fill="#fff700"/>
    <circle cx="0" cy="-40" r="3" fill="#e17055"/>
  </g>
  
  <!-- Hibiscus secundario más pequeño -->
  <g transform="translate(320,100) scale(0.6)">
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(0)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(72)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(144)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(216)"/>
    <ellipse cx="0" cy="-50" rx="30" ry="55" fill="url(#hibiscusPetal)" transform="rotate(288)"/>
    <circle cx="0" cy="0" r="15" fill="url(#hibiscusCenter)"/>
    <rect x="-2" y="-40" width="4" height="40" fill="#d63031"/>
    <circle cx="0" cy="-40" r="6" fill="#fff700"/>
  </g>
  
  <!-- Hojas grandes del hibiscus -->
  <g transform="translate(120,200)">
    <path d="M 0,0 Q 30,-20 60,0 Q 30,40 0,0" fill="#27ae60"/>
    <path d="M 5,5 Q 30,-10 55,5 Q 30,35 5,5" fill="#2ed573"/>
  </g>
  
  <g transform="translate(250,220)">
    <path d="M 0,0 Q -25,-15 -50,0 Q -25,35 0,0" fill="#27ae60"/>
    <path d="M -5,5 Q -25,-5 -45,5 Q -25,30 -5,5" fill="#2ed573"/>
  </g>
  
  <!-- Tallo -->
  <rect x="195" y="200" width="10" height="70" fill="#27ae60"/>
  
  <!-- Botones florales -->
  <ellipse cx="100" cy="80" rx="8" ry="15" fill="#d63031" transform="rotate(-30 100 80)"/>
  <ellipse cx="300" cy="60" rx="6" ry="12" fill="#e17055" transform="rotate(45 300 60)"/>
  
  <!-- Efectos de brillo -->
  <circle cx="180" cy="120" r="3" fill="#ffffff" opacity="0.7"/>
  <circle cx="250" cy="100" r="2" fill="#fff700" opacity="0.8"/>
  <circle cx="160" cy="180" r="2" fill="#ffffff" opacity="0.6"/>
</svg>
